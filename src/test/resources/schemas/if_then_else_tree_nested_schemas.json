{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "tree-nested": {
      "type": "integer",
      "minimum": 1,
      "maximum": 1000,
      "if": {
        "minimum": 1,
        "maximum": 500
      },
      "then": {
        "if": {
          "minimum": 1,
          "maximum": 250
        },
        "then": {
          "multipleOf": 2
        },
        "else": {
          "multipleOf": 5
        }
      },
      "else": {
        "if": {
          "minimum": 500,
          "maximum": 750
        },
        "then": {
          "multipleOf": 20
        },
        "else": {
          "multipleOf": 50
        }
      }
    }
  }
}