{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "array-strings": {
      "type": "array",
      "items": { "type": "string" }
    },
    "array-integers": {
      "type": "array",
      "items": { "type": "integer" }
    },
    "array-booleans": {
      "type": "array",
      "items": { "type": "boolean" }
    },
    "array-enum": {
      "type": "array",
      "items": {
        "enum": ["ENUM_01", "ENUM_02", "ENUM_03", "ENUM_04", "ENUM_05"]
      }
    },
    "array-objects": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "string": { "type": "string" },
          "number": { "type": "number"}
        }
      }
    },
    "array-single-tuple": {
      "type": "array",
      "items": [
        { "type": "string" }
      ]
    },
    "min-max-items": {
      "type": "array",
      "minItems": 5,
      "maxItems": 10,
      "items": { "type": "integer" }
    },
    "min-equal-max-items": {
      "type": "array",
      "minItems": 5,
      "maxItems": 5,
      "items": { "type": "integer" }
    },
    "unique-items": {
      "type": "array",
      "items": {
        "enum": ["ENUM_01", "DUPLICATED", "DUPLICATED", "DUPLICATED"]
      },
      "uniqueItems": true
    }
  }
}