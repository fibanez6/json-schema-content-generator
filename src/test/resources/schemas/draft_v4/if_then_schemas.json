{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "text": {
      "type": "string",
      "if": {
        "minLength": 2,
        "maxLength": 5
      },
      "then": {
        "pattern": "[A-F]{5}"
      }
    }
  }
}
