{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nested": {
      "type": "integer",
      "minimum": 1,
      "maximum": 1000,
      "if": {
        "minimum": 100
      },
      "then": {
        "multipleOf": 100
      },
      "else": {
        "if": {
          "minimum": 10
        },
        "then": {
          "multipleOf": 10
        }
      }
    },
    "nested-reverted": {
      "type": "integer",
      "minimum": 1,
      "maximum": 1000,
      "if": {
        "minimum": 10
      },
      "then": {
        "multipleOf": 10
      },
      "else": {
        "if": {
          "minimum": 100
        },
        "then": {
          "multipleOf": 100
        }
      }
    }
  }
}