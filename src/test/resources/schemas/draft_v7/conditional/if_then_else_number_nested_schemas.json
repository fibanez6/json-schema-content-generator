{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "multi-nested": {
      "type": "integer",
      "minimum": 1,
      "maximum": 100000,
      "if": {
        "minimum": 10000
      },
      "then": {
        "multipleOf": 10000
      },
      "else": {
        "if": {
          "minimum": 1000
        },
        "then": {
          "multipleOf": 1000
        },
        "else": {
          "if": {
            "minimum": 100
          },
          "then": {
            "multipleOf": 100
          },
          "else": {
            "if": {
              "minimum": 10
            },
            "then": {
              "multipleOf": 10
            },
            "else": {
              "multipleOf": 1
            }
          }
        }
      }
    }
  }
}